{
  "name": "@ohos/oaid",
  "description": "Allowing apps to use OS OAID get/set functions",
  "version": "3.2",
  "license": "Apache License 2.0",
  "publishAs": "code-segment",
  "segment": {
    "destPath": "domains/cloud/oaid"
  },
  "dirs": {},
  "scripts": {},
  "component": {
    "name": "oaid",
    "subsystem": "cloud",
    "syscap": [
      "SystemCapability.Cloud.OAID"
    ],
    "features": [
    ],
    "adapted_system_type": [
      "standard"
    ],
    "rom":"300KB",
    "ram":"1024KB",
    "deps": {
      "components": [
        "ability_manager",
        "aafwk_standard",
        "appexecfwk_standard",
        "hiviewdfx_hilog_native",
        "kv_store",
        "ipc",
        "permission_standard",
        "safwk",
        "samgr",
        "startup_l2",
        "utils_base"
      ],
      "third_party": [
        "jsoncpp"
      ]
    },
    "build": {
      "sub_component": [
        "//domains/cloud/oaid:oaid_native_packages"
      ],
      "test": [
      ]
    }
  }
}
